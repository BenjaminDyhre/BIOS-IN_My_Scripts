[bsdyhre@login-5.SAGA /cluster/projects/nn9986k/BIOS5410/sbatch_submission]$ cat test_script.sh
#!/bin/bash

#SBATCH --job-name=test_job     #give jobs a name
#SBATCH --account=nn9986k       #current project that we are on
#SBATCH --nodes=1               #number of nodes/CPU
#SBATCH --ntasks=1              #how many tasks to run simutanously
#SBATCH --time=3:00:00          #how long the job will go for
#SBATCH --mem=1G                #how much memory

## Set up job environment
set -o errexit                  # Exit script on any error
set -o nounset                  # Treat any unset variables as an error

module --quiet purge            #this resets the module environment, always good to start clean!

## DO SOMETHING
## Copy some input files to the work directory:
touch file.txt          #create some empty file
cp $SUBMITDIR/file.txt $SCRATCH

## Make a directory in the directory from WHERE you submitted the script
mkdir -p $SUBMITDIR/result      #create a results directory

## Copy output files to where you started the script
cp $SCRATCH/file.txt  $SUBMITDIR/result/

## Remove the file.txt from the $SUBMITDIR  (you're still there)
rm file.txt

## Display a message
echo "Ran a job on file.txt!"

## move to the job_specific work directory
cd $SCRATCH

## Message that you are done with the job
echo "Finished running jobs"
